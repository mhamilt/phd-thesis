	keywords = {Auditory perception ; Cognition ; Communication ; Cues ; Decision making ; Evaluation ; Evidence Expert ; Female ; Human beings ; Information resources ; Judgment ; Judgments ; Male ; Music ; Musicians ; Photic Stimulation ; Social perception ; Social psychology ; Social sciences ; Sound ; Visual perception},
	language = {eng},
	number = {36},
	pages = {14580-14585},
	publisher = {National Academy of Sciences},
	title = {Sight over sound in the judgment of music performance},
	volume = {110},
	year = {2013}}

