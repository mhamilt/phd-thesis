	isbn = {9781450348225},
	keywords = {Engineering ; Struggle ; Technology},
	language = {eng},
	organization = {ACM},
	pages = {116-123},
	publisher = {ACM},
	title = {Effect of latency on performer interaction and subjective quality assessment of a digital musical instrument},
	volume = {4-06-},
	year = {2016}}

